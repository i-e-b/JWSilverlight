<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:jwSkinControls_ControlFragments="clr-namespace:jwSkinControls.ControlFragments"
	mc:Ignorable="d"
	x:Class="jwSkinControls.JW5_Playlist"
	d:DesignWidth="640" d:DesignHeight="480" Width="200">

	<Grid x:Name="LayoutRoot">
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
			<ColumnDefinition Width="Auto"/>
		</Grid.ColumnDefinitions>
		<StackPanel x:Name="PlaylistItemStack" Margin="0"/>
		<jwSkinControls_ControlFragments:JwSliderVertical x:Name="ScrollSlider" Grid.Column="1" Margin="0" Width="10" HorizontalAlignment="Left"/>
	</Grid>
</UserControl>